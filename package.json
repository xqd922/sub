{
  "name": "sub",
  "version": "1.0.0",
  "private": true,
  "description": "订阅转换服务 - 支持 Clash、Sing-box 等代理客户端的订阅链接转换",
  "author": "",
  "license": "MIT",
  "packageManager": "bun@latest",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "subscription",
    "converter",
    "clash",
    "sing-box",
    "proxy",
    "vpn",
    "next.js"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "dev:cf": "bunx --bun wrangler pages dev --port 3000 --persist-to .wrangler/state .next",
    "dev:fast": "next dev --turbo --no-lint --no-type-check",
    "build": "next build",
    "pages:build": "bun x @cloudflare/next-on-pages",
    "pages:deploy": "bun run build && bun run pages:build",
    "start": "next start",
    "lint": "next lint",
    "package": "bun scripts/package.js"
  },
  "dependencies": {
    "@heroui/react": "^3.0.0-beta.3",
    "@heroui/styles": "^3.0.0-beta.3",
    "@tailwindcss/postcss": "^4.1.18",
    "caniuse-lite": "^1.0.30001760",
    "js-yaml": "^4.1.1",
    "next": "16.0.8",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20251225.0",
    "@eslint/eslintrc": "^3.3.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.26",
    "@types/react": "^19.2.7",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "wrangler": "^4.59.2"
  }
}
